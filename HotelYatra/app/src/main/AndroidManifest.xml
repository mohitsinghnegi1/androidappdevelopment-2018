<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hotelyatra">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_hotel_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Second" />
        <activity
            android:name=".landingpage"
            android:label="@string/title_activity_landingpage"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".bookNow"
            android:label="@string/title_activity_book_now"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".payment"
            android:label="@string/title_activity_payment"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Information"
            android:label="@string/title_activity_information"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".ourservices"
            android:label="@string/title_activity_ourservices"
            android:theme="@style/AppTheme" />
        <activity android:name=".login" />
        <activity android:name=".Signup"></activity>
    </application>

</manifest>